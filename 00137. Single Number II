class Solution 
{
    public int singleNumber(int[] nums) 
    {
        int res=0;
        int n=nums.length;
        for(int i=0;i<32;i++)
        {
            int count=0;
            for(int j=0;j<n;j++)
            {
                if(checkBit(nums[j],i)==true)
                {
                    count++;
                }
            }
            if(count%3!=0)
                res|=1<<i;
        }
        return res;
    }
    public boolean checkBit(int a, int k) {
        return (a & (1 << k)) != 0;
    }
}
